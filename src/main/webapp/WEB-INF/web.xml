<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" version="3.1">
    <servlet>
    <servlet-name>TeamServlet</servlet-name>
    <servlet-class>com.dataware.controller.TeamServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>TeamServlet</servlet-name>
    <url-pattern>/teams/team</url-pattern>
</servlet-mapping>

    <servlet>
    <servlet-name>MemberServlet</servlet-name>
    <servlet-class>com.dataware.controller.MemberServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>MemberServlet</servlet-name>
    <url-pattern>/members/member</url-pattern>
</servlet-mapping>

    <servlet>
    <servlet-name>MemberTeamServlet</servlet-name>
    <servlet-class>com.dataware.controller.MemberTeamServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>MemberTeamServlet</servlet-name>
    <url-pattern>/member-team</url-pattern>
</servlet-mapping>


   <servlet>
        <servlet-name>TaskServlet</servlet-name>
        <servlet-class>com.dataware.controller.TaskControllerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TaskServlet</servlet-name>
        <url-pattern>/tasks</url-pattern>
    </servlet-mapping>
    
    
    
</web-app>